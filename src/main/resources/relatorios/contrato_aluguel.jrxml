	<parameter name="contrato" class="java.lang.Long"/>
	<parameter name="subreport" class="java.lang.String"/>
	<queryString language="SQL">
		<![CDATA[select a.id, a.dataInicio, a.dataPrevista, a.valorTotal, a.valorLuva,
cli.nome, cli.documentoReceita, cli.rg,
concat(concat(concat(concat(cli.logradouro, ' ', cli.numero),' - ', cli.bairro),', ', cli.cidade), '/', cli.estado) endereco,
v.chassi, v.placa, v.anoFabricacao, a.kmInicial, m.descricao modelo, m.motorizacao, f.descricao fabricante
-- , it.descricao checklist
from alugueis a
inner join pessoas cli on a.cliente_id = cli.id
inner join veiculos v on a.veiculo_id = v.id
inner join modelos m on v.modelo_id = m.id
inner join fabricantes f on m.fabricante_id = f.id
-- inner join checklist_contrato cc on a.id = cc.aluguel_id
-- inner join itens_check_list it on cc.item_checklist_id = it.id
where a.id = $P{contrato} 
--  and cc.entrega = 1]]>
	</queryString>
