	<queryString>
		<![CDATA[select
  m.id,
  p.nome,
  p.logradouro,
  p.numero,
  p.cidade,
  p.estado,
  m.entrada,
  f.descricao fabricante,
  concat(mo.descricao, ' ', mo.motorizacao) modeloMotorizacao,
  v.combustivel,
  concat(v.anoFabricacao, '/', v.anoModelo) anoModelo,
  v.placa,
  v.quilometragem
