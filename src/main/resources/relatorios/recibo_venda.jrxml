inner join pessoas p on m.cliente_id = p.id
inner join veiculos v on m.veiculo_id = v.id
inner join modelos mo on v.modelo_id = mo.id
inner join fabricantes f on mo.fabricante_id = f.id
where m.id = $P{id}]]>
